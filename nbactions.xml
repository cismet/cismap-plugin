        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:1.1.1:exec</goal>
        </goals>
        <properties>
            <exec.args>-classpath %classpath de.cismet.cismap.navigatorplugin.CismapPlugin</exec.args>
