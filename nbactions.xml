        <goals>
            <goal>deploy</goal>
        </goals>
    </action>
    <action>
        <actionName>CUSTOM-public-deploy-flexo</actionName>
        <displayName>public-deploy-flexo</displayName>
        <goals>
            <goal>clean</goal>
            <goal>deploy</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>deploy</activatedProfile>
            <activatedProfile>deploy-public</activatedProfile>
        </activatedProfiles>
    </action>

